    rdfs:comment "Basic workflow ontology for modeling derivation graphs and provenance relations in GIS"@en ;
    owl:imports <http://geographicknowledge.de/vocab/AnalysisData.rdf> .

wf:edge a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "A property that links to the (reified) statements of a workflow (workflow edges). Use rdf:subject, rdf:predicate and rdf:object to further link the statement with its elements." ;
    rdfs:domain wf:Workflow .

rdf:first a rdf:Property ;
    rdfs:subPropertyOf rdf:first .

rdf:rest a rdf:Property ;
    rdfs:subPropertyOf rdf:rest .

rdf:type a rdf:Property ;
    rdfs:subPropertyOf rdf:type .

rdfs:comment a rdf:Property ;
    rdfs:subPropertyOf rdfs:comment .

rdfs:domain a rdf:Property ;
    rdfs:subPropertyOf rdfs:domain .

rdfs:range a rdf:Property ;
    rdfs:subPropertyOf rdfs:range .

rdfs:subClassOf a rdf:Property ;
    rdfs:subPropertyOf rdfs:subClassOf .

rdfs:subPropertyOf a rdf:Property ;
    rdfs:subPropertyOf rdfs:subPropertyOf .

owl:FunctionalProperty a rdfs:Resource .

owl:disjointWith a rdf:Property ;
    rdfs:subPropertyOf owl:disjointWith .

owl:equivalentClass a rdf:Property ;
    rdfs:subPropertyOf owl:equivalentClass .

owl:imports a rdf:Property ;
    rdfs:subPropertyOf owl:imports .

owl:intersectionOf a rdf:Property ;
    rdfs:subPropertyOf owl:intersectionOf .

owl:inverseOf a rdf:Property ;
    rdfs:subPropertyOf owl:inverseOf .

owl:onProperty a rdf:Property ;
    rdfs:subPropertyOf owl:onProperty .

owl:oneOf a rdf:Property ;
    rdfs:subPropertyOf owl:oneOf .

owl:propertyChainAxiom a rdf:Property ;
    rdfs:subPropertyOf owl:propertyChainAxiom .

owl:someValuesFrom a rdf:Property ;
    rdfs:subPropertyOf owl:someValuesFrom .

owl:topObjectProperty a rdfs:Resource .

<http://geographicknowledge.de/vocab/AnalysisData.rdf> a rdfs:Resource,
        owl:Ontology ;
    rdfs:comment "This vocabulary is a design pattern to be used for describing analysis data sets on a level that allows to make inferences about the kind of analysis applicable to the dataset"@en .

ada:Interval a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:Nominal,
        ada:Ordinal,
        ada:RefSys,
        ada:ScaleLevel .

ada:SpaceRS a rdfs:Resource,
        owl:Class ;
    rdfs:comment "Spatial reference systems"@en ;
    rdfs:subClassOf ada:RefSys,
        ada:RefType .

ada:WGS84 a ada:Interval,
        ada:Nominal,
        ada:Ordinal,
        ada:RefSys,
        ada:RefType,
        ada:ScaleLevel,
        ada:SpaceRS,
        rdfs:Resource,
        owl:NamedIndividual,
        owl:Thing .

ada:elementOf a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "The relation between a data item and its data set"@en ;
    rdfs:domain ada:Data ;
    rdfs:range ada:DataSet .

gis:Distance a rdfs:Resource,
        owl:Class ;
    rdfs:comment "a quality that denotes the spatial distance to a set of objects"@en ;
    rdfs:subClassOf _:Nae2bfea6eed341c6a635e0e24d44a414,
        ada:Quality,
        ada:Reference,
        gis:RelQ,
        wf:Resource .

gis:Point a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:Reference,
        ada:Spatial,
        wf:Resource .

gis:Region a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:Reference,
        ada:Spatial,
        wf:Resource .

gis:Table a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Fun,
        wf:Operation,
        wf:Resource .

ada:Ordinal a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:Nominal,
        ada:RefSys,
        ada:ScaleLevel .

gis:LocalMapAlgebra a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Fun,
        gis:RasterOp,
        wf:Operation,
        wf:Resource .

gis:Path a rdfs:Resource,
        owl:Class ;
    rdfs:comment "A single element of a spatial network denoting a spatial path from start object to end object"@en ;
    rdfs:subClassOf [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty gis:hasStart ;
            owl:someValuesFrom ada:Object ],
        [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty gis:hasEnd ;
            owl:someValuesFrom ada:Object ],
        ada:Object,
        ada:Reference,
        wf:Resource .

gis:VectorTopology a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Fun,
        wf:Operation,
        wf:Resource .

gis:in a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "input of a function" ;
    rdfs:domain gis:Fun .

gis:out a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:domain gis:Fun .

gis:toRaster a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Conversion,
        gis:Fun,
        wf:Operation,
        wf:Resource .

gis:toVector a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Conversion,
        gis:Fun,
        wf:Operation,
        wf:Resource .

owl:NamedIndividual a rdfs:Resource .

owl:Ontology a rdfs:Resource .

ada:SpatialData a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf _:Na0880313f9ff40ff94b3fba5937ee3f8,
        _:Ndd7d74a866f34adda31199ee1ea83d7b,
        _:Ne67acbc312de45b8b9277623153140e9,
        ada:Data,
        wf:Resource ;
    owl:equivalentClass [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty ada:hasSupport ;
            owl:someValuesFrom ada:Spatial ] .

ada:SpatialDataSet a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:DataSet,
        wf:Resource ;
    owl:equivalentClass [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty ada:hasElement ;
            owl:someValuesFrom ada:SpatialData ] .

wf:Workflow a rdfs:Resource,
        owl:Class ;
    rdfs:comment "A class for instances that denote workflows. Workflow instances should be connected to triples of the workflow, using RDF reification."@en .

ada:Nominal a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:RefSys,
        ada:ScaleLevel .

ada:RefType a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf ada:RefSys .

gis:inputdata a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "Whenever the input is data, and not a single value or a function" ;
    rdfs:subPropertyOf wf:input .

ada:ScaleLevel a rdfs:Resource,
        owl:Class ;
    rdfs:comment "These levels correspond to Stevensons scale levels and could be extended by further ones."@en ;
    rdfs:subClassOf ada:RefSys .

ada:Temporal a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of temporal referents defined in some temporal reference system"@en ;
    rdfs:subClassOf ada:Reference,
        wf:Resource ;
    owl:equivalentClass [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty ada:referencedBy ;
            owl:someValuesFrom ada:TimeRS ] .

owl:Thing a rdfs:Resource,
        owl:Class .

ada:Spatial a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of spatial referents defined in some spatial reference system"@en ;
    rdfs:subClassOf ada:Reference,
        wf:Resource ;
    owl:equivalentClass [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty ada:referencedBy ;
            owl:someValuesFrom ada:SpaceRS ] .

ada:referencedBy a rdf:Property,
        rdfs:Resource,
        owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "The relation between a reference and its reference system. For example, a reference would be 50 and the reference system  may be Celsius"@en ;
    rdfs:domain ada:Reference ;
    rdfs:range ada:RefSys ;
    rdfs:subPropertyOf ada:referencedBy .

gis:RelQ a rdfs:Resource,
        owl:Class ;
    rdfs:comment "A relational quality"@en ;
    rdfs:subClassOf _:Nae2bfea6eed341c6a635e0e24d44a414,
        ada:Quality,
        ada:Reference,
        wf:Resource .

wf:input a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:domain wf:Operation .

wf:of a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "An overloaded property for linking resources inside a workflow back with provenance resources"@en .

ada:hasElement a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "the inverse of elementOf"@en ;
    owl:inverseOf ada:elementOf .

ada:hasMeasure a rdf:Property,
        rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "The relation between a data item and its measure"@en ;
    rdfs:domain ada:Data ;
    rdfs:range ada:Reference ;
    rdfs:subPropertyOf ada:hasMeasure .

gis:Conversion a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Fun,
        wf:Operation,
        wf:Resource .

gis:RasterOp a rdfs:Resource,
        owl:Class ;
    rdfs:subClassOf gis:Fun,
        wf:Operation,
        wf:Resource .

() a rdfs:Resource .

ada:Object a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of discrete entities (includes objects (endurants) as well as events (occurrents))"@en ;
    rdfs:subClassOf ada:Reference,
        wf:Resource ;
    owl:equivalentClass [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty ada:referencedBy ;
            owl:someValuesFrom ada:ObjectRS ] .

ada:hasSupport a rdfs:Resource,
        owl:ObjectProperty ;
    rdfs:comment "The relation between a data item and its support"@en ;
    rdfs:domain ada:Data ;
    rdfs:range ada:Reference .

ada:DataSet a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of data sets (collections of data items)"@en ;
    rdfs:subClassOf wf:Resource ;
    owl:disjointWith ada:RefSys,
        ada:Reference .

ada:Quality a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of values of some quality"@en ;
    rdfs:subClassOf ada:Reference,
        wf:Resource ;
    owl:equivalentClass [ a rdfs:Resource,
                owl:Restriction ;
            owl:onProperty ada:referencedBy ;
            owl:someValuesFrom ada:QualityRS ] .

ada:RefSys a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of reference systems"@en ;
    owl:disjointWith ada:Reference .

ada:Data a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of data items. Data items bind together different attributes of an observation, just like table records or primary keys."@en ;
    rdfs:subClassOf _:Na0880313f9ff40ff94b3fba5937ee3f8,
        _:Ndd7d74a866f34adda31199ee1ea83d7b,
        _:Ne67acbc312de45b8b9277623153140e9,
        wf:Resource ;
    owl:disjointWith ada:DataSet,
        ada:RefSys,
        ada:Reference .

ada:Reference a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of symbols that refer to observable phenomena and belong to a reference system"@en ;
    rdfs:subClassOf wf:Resource .

wf:Operation a rdfs:Resource,
        owl:Class ;
    rdfs:comment "GIS Operations "@en,
        "The event of applying some tool to inputs generating outputs inside a workflow"@en ;
    rdfs:subClassOf gis:Fun,
        wf:Resource .

gis:Fun a rdfs:Resource,
        owl:Class ;
    rdfs:comment "The class of functions. This is a general notion for any operation in and outside a database. Has input and output."@en ;
    rdfs:subClassOf wf:Resource .

owl:ObjectProperty a rdfs:Resource .

owl:Restriction a rdfs:Resource .

wf:Resource a rdfs:Resource,
        owl:Class ;
    rdfs:comment "Any resource used in a workflow. May be a function or a dataset or data or a reference" .

owl:Class a rdfs:Resource .

_:Nae2bfea6eed341c6a635e0e24d44a414 a rdfs:Resource,
        owl:Restriction ;
    owl:onProperty wf:of ;
    owl:someValuesFrom ada:DataSet .

_:Na0880313f9ff40ff94b3fba5937ee3f8 a rdfs:Resource,
        owl:Restriction ;
    owl:onProperty ada:hasMeasure ;
    owl:someValuesFrom owl:Thing .

_:Ndd7d74a866f34adda31199ee1ea83d7b a rdfs:Resource,
        owl:Restriction ;
    owl:onProperty ada:hasSupport ;
    owl:someValuesFrom owl:Thing .

_:Ne67acbc312de45b8b9277623153140e9 a rdfs:Resource,
        owl:Restriction ;
    owl:onProperty ada:elementOf ;
    owl:someValuesFrom owl:Thing .


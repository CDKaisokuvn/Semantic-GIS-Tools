@prefix : <http://geographicknowledge.de/WorkflowExamples#>.

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix wf: <http://geographicknowledge.de/vocab/Workflow.rdf#>.
@prefix gis: <http://geographicknowledge.de/vocab/GISConcepts.rdf#>.
@prefix ada: <http://geographicknowledge.de/vocab/AnalysisData.rdf#>.

# Workflow of China Lights analysis
# @author: Andrea Ballatore, Simon Scheider

# Workflow metadata (result and data sources)
:wf2 a wf:Workflow.
:wf2 rdfs:comment "An example of a workflow. It analyses satellite imagery to estimate the change in luminosity in China.".
:wf2 wf:source :F101993_night_lights.
:wf2 wf:source :F102013_night_lights.
:wf2 wf:source :China_border.
:wf2 wf:source :Gas_flares.
:wf2 wf:source :Train_stations.
:wf2 wf:source :Admin_units.
:wf2 wf:result :Luminosity_change.

#:F101993_night_lights a wf:Resource.
#:F102013_night_lights a wf:Resource.
#:China_border a wf:Resource.
#:Gas_flares a wf:Resource.
#:Train_stations a wf:Resource.
#:Admin_units a wf:Resource.
#:Luminosity_change a wf:Resource.

# Workflow Node 1
:wf2_1 gis:inputdata :China_border;
	gis:erasefeature :Gas_flares;
	wf:output :Chn_atm0_erase;
	rdf:type gis:Erase.

# Workflow Node 2
:wf2_2 gis:inputdata :Train_stations;
	wf:output :Ch_Stations_Buffer;
	rdf:type gis:Buffer.

# Workflow Node 3
:wf2_3 rdf:type gis:Intersect;
	gis:inputdata :Chn_atm0_erase;
	gis:inputdata :Ch_Stations_Buffer; # RENAME PARAMETER?
	wf:output :CHN_adm0_Erase_Intersect.

# Workflow Node 4
:wf2_4 rdf:type gis:FeatureToRaster;
	gis:inputdata :CHN_adm0_Erase_Intersect;
	wf:output :Feature_Chn_1.

# Workflow Node 5
:wf2_5 rdf:type gis:ExtractByMask; # ADD TO ONTO
	gis:inputdata :F102013_night_lights;
	gis:inputdata :Feature_Chn_1; # MASK, RENAME PARAM
	wf:output :Extract_tif2.

# Workflow Node 6
:wf2_6 rdf:type gis:ExtractByMask; # ADD TO ONTO
	gis:inputdata :F101993_night_lights;
	gis:inputdata :Feature_Chn_1; # MASK, RENAME PARAM
	wf:output :Extract_tif1.

# Workflow Node 7
:wf2_7 rdf:type gis:ZonalStatisticsAsTable; # ADD TO ONTO
	gis:inputdata :Extract_tif1;
	gis:inputdata :Admin_units; # ZONES, RENAME PARAM
	wf:output :ZonalSt_shp2.

# Workflow Node 8
:wf2_8 rdf:type gis:ZonalStatisticsAsTable; # ADD TO ONTO
	gis:inputdata :Extract_tif2;
	gis:inputdata :Admin_units; # ZONES, RENAME PARAM
	wf:output :ZonalSt_shp1.

# Workflow Node 9
:wf2_9 rdf:type gis:JoinField; # ADD TO ONTO
	gis:inputdata :ZonalSt_shp1;
	gis:inputdata :ZonalSt_shp2; # RENAME PARAM
	wf:output :ZonalSt_shp1_1.

# Workflow Node 10
:wf2_10 rdf:type gis:CalculateField; # ADD TO ONTO
	gis:inputdata :ZonalSt_shp1_1;
	wf:output :Luminosity_change.
